{
  "name": "take-away-as",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=dev PORT=3001 node dist/bin/www",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "test": "cross-env NODE_ENV=test mocha --timeout 6000 test/functional/api/*.js --exit",
    "test:seller": "cross-env NODE_ENV=test mocha --timeout 6000 test/functional/api/test-seller.js --exit",
    "test:user": "cross-env NODE_ENV=test mocha --timeout 6000 test/functional/api/test-user.js --exit",
    "test:order": "cross-env NODE_ENV=test mocha --timeout 6000 test/functional/api/test-order.js --exit",
    "postbuild": "npm start",
    "prebuild": "npm-run-all test clean",
    "build": "npm run compile",
    "clean": "rimraf ./dist && mkdir dist",
    "compile": "babel ./ --out-dir dist/ --ignore ./node_modules,./test --copy-files",
    "lint": "esw  ./test ./routes ./models/ app.js bin/www ",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "cookie-parser": "~1.4.4",
    "cross-env": "^6.0.3",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1",
    "sha1": "^1.1.1",
    "stylus": "0.54.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.0.2",
    "eslint": "^6.5.1",
    "eslint-watch": "^6.0.1",
    "mocha": "^6.2.0",
    "mongodb-memory-server": "^5.2.7",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
